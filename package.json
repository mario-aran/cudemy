{
  "name": "udemy",
  "private": true,
  "type": "module",
  "scripts": {
    "dev:services": "pnpm exec concurrently \"pnpm --filter services/courses run dev\" \"pnpm --filter services/media run dev\"",
    "clean": "find . -type d \\( -name 'node_modules' -o -name 'dist' -o -name 'coverage' \\) -prune -exec rm -rf {} +",
    "format:eol": "find . -type f -exec dos2unix {} +",
    "format": "prettier --write .",
    "lint": "tsc --noEmit && eslint . --fix"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@scope/prettier-config": "workspace:*",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-check-file": "^3.3.1",
    "globals": "^16.5.0",
    "prettier": "^3.6.2",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.3"
  }
}
